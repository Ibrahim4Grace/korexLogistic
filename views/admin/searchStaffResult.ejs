<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Korex Logistics</title>
    <!-- base:css -->
    <link rel="stylesheet" href="../font/typicons.css">
    <link rel="stylesheet" href="../css/vendor.bundle.base.css">
    <link rel="stylesheet" href="../vertical-layout-light/style.css">
    <!-- endinject -->
    <link rel="icon" href="../img/favi.jpg">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.1.2/socket.io.js"></script>
  </head>
  <body>
    <div class="container-scroller">
   
        <%- include ('../partials/adminNav'); -%>
    
      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <!-- partial:partials/_settings-panel.html -->
        <div class="theme-setting-wrapper">
          <div id="settings-trigger"><i class="typcn typcn-cog-outline"></i></div>
          <div id="theme-settings" class="settings-panel">
            <i class="settings-close typcn typcn-delete-outline"></i>
            <p class="settings-heading">SIDEBAR SKINS</p>
            <div class="sidebar-bg-options" id="sidebar-light-theme">
              <div class="img-ss rounded-circle bg-light border mr-3"></div>
              Light
            </div>
            <div class="sidebar-bg-options selected" id="sidebar-dark-theme">
              <div class="img-ss rounded-circle bg-dark border mr-3"></div>
              Dark
            </div>
            <p class="settings-heading mt-2">HEADER SKINS</p>
            <div class="color-tiles mx-0 px-4">
              <div class="tiles success"></div>
              <div class="tiles warning"></div>
              <div class="tiles danger"></div>
              <div class="tiles primary"></div>
              <div class="tiles info"></div>
              <div class="tiles dark"></div>
              <div class="tiles default border"></div>
            </div>
          </div>
        </div>
        <!-- partial -->
        <!-- partial:partials/_sidebar.html -->
        <%- include('../partials/adminSidebar'); -%>

        <!-- partial -->
        <div class="main-panel">
            <div class="content-wrapper">

              
                <%- include (`../partials/messages`); -%>

                   
                <div class="col-lg-12 grid-margin stretch-card">
                    <div class="card">
                      <div class="card-body">
                        <h4 class="card-title">Shipping History</h4>
                            <div class="houseSearch">
                              <form action="/admin/searchStaffResult" method="post">
                                <input class="houseSearch" type="text" name="staffName" placeholder="Search Staff Name...">
                              </form>
                            </div>
                        
                        <div class="table-responsive pt-3">
                          <table class="table table-bordered">
                            <thead>
                              <tr>
                                  <th>Staff Image</th>
                                  <th>Staff Name</th>
                                  <th>Staff Number</th>
                                  <th>Staff Position</th>
                                  <th>Staff Address</th>
                                  <th>Staff State</th>
                                  <th>Emergency Name</th>
                                  <th>Staff Details</th>
                                  <th>Edit</th>
                                  <th>Delete</th>
                              </tr>
                           </thead>
                          <tbody>
                            <% if (korexStaffs.length > 0) { %>
                            <% korexStaffs.forEach(korexStaff => { %>
                                <tr  class="center">
                                  <td>
                                    <% if (korexStaff.image && korexStaff.image.data) { %>
                                      <img src="data:<%= korexStaff.image.contentType %>;base64,<%= korexStaff.image.data.toString('base64') %>" alt="User Image">
                                    <% } else { %>
                                      NA
                                    <% } %>
                                  </td>
                                  <td> <%= korexStaff.staffName %></td>
                                  <td> <%= korexStaff.staffNumber %></td>
                                  <td> <%= korexStaff.staffPosition %></td>
                                  <td> <%= korexStaff.staffAddress %></td>
                                  <td> <%= korexStaff.staffState %></td>
                                  <td> <%= korexStaff.staffEmergencyName %></td>

                                    <td data-label="Profile">
                                        <a href="/admin/staffProfile/<%- korexStaff._id %>">
                                            View
                                        </a>
                                    </td>
                                    <td data-label="Edit">
                                        <a href="/admin/editStaff/<%- korexStaff._id %>">
                                            <i class="typcn typcn-pencil menu-icon" style="color: blue;"></i>
                                      </a>
                                    </td>
                                    <td data-label="Delete">
                                      <a href="/admin/deleteStaff/<%- korexStaff._id %>">
                                        <i class="typcn typcn-trash menu-icon" style="color: blue;"></i>
                                      </a>
                                    </td>
      
                                </tr>
                                <% }); %>
                                <% } else { %>
                                    <tr>
                                        <td colspan="9" style="text-align: center;">No Staff found with the name.</td>
                                    </tr>
                                <% } %>
                          </tbody>
                          </table>
                          <div class="pagination">
                            <% for (let i=1; i <=totalPages; i++) { %>
                                <a href="/admin/searchUserResult?page=<%= i %>"
                                    class="<%= currentPage === i ? 'active' : '' %>">
                                    <%= i %>
                                </a>
                                <% } %>
                          </div>
                        </div>
                      </div>
                    </div>
                </div>
  
                
              


                  
          

        
            </div>
      
            <%- include('../partials/adminFooter'); -%>
           
     
        </div>
   
      </div>
  
    </div>


  </body>
</html>