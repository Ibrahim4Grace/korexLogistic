<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Korex Hospital</title>
    <!-- base:css -->
    <link rel="stylesheet" href="../../font/typicons.css">
    <link rel="stylesheet" href="../../css/vendor.bundle.base.css">
    <link rel="stylesheet" href="../../vertical-layout-light/style.css">
    <!-- endinject -->
    <link rel="icon" href="../../img/favi.jpg">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.1.2/socket.io.js"></script>
  </head>
  <body>
    
        <%- include ('../partials/adminNav'); -%>

      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <!-- partial:partials/_settings-panel.html -->
        <div class="theme-setting-wrapper">
          <div id="settings-trigger"><i class="typcn typcn-cog-outline"></i></div>
          <div id="theme-settings" class="settings-panel">
            <i class="settings-close typcn typcn-delete-outline"></i>
            <p class="settings-heading">SIDEBAR SKINS</p>
            <div class="sidebar-bg-options" id="sidebar-light-theme">
              <div class="img-ss rounded-circle bg-light border mr-3"></div>
              Light
            </div>
            <div class="sidebar-bg-options selected" id="sidebar-dark-theme">
              <div class="img-ss rounded-circle bg-dark border mr-3"></div>
              Dark
            </div>
            <p class="settings-heading mt-2">HEADER SKINS</p>
            <div class="color-tiles mx-0 px-4">
              <div class="tiles success"></div>
              <div class="tiles warning"></div>
              <div class="tiles danger"></div>
              <div class="tiles primary"></div>
              <div class="tiles info"></div>
              <div class="tiles dark"></div>
              <div class="tiles default border"></div>
            </div>
          </div>
        </div>
        <!-- partial -->
     
        <%- include('../partials/adminSidebar'); -%>

        <!-- partial -->
        <div class="main-panel">
          <div class="content-wrapper">

              
                <%- include (`../partials/messages`); -%>

                   
          
            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Admin Profile</h4>

                        <ul class="v">
                            <li><strong>Image</strong></li>
                            <p>
                            <img class="imageSize" src="data:<%= adminInfo.image.contentType %>;base64,
                               <%= adminInfo.image.data.toString('base64') %>" alt="Admin Image" loading="lazy">
                            </p>
                            <li><strong>Admin Name</strong></li>
                            <p>
                                <%=adminInfo.adminFullName%>
                            </p>
                            <li><strong>Username</strong></li>
                            <p>
                                <%=adminInfo.adminUsername%>
                            </p>
                            <li><strong>Email Address</strong></li>
                            <p>
                                <%=adminInfo.adminEmail%>
                            </p>
                            <li><strong>Phone Number</strong></li>
                            <p>
                                <%=adminInfo.adminNumber%>
                            </p>
                            <li><strong>Address</strong></li>
                            <p>
                                <%=adminInfo.adminAddress%>
                            </p>
                            <li><strong>City</strong></li>
                            <p>
                                <%=adminInfo.adminCity%>
                            </p>
                            <li><strong>State</strong></li>
                            <p>
                                <%=adminInfo.adminState%>
                            </p>
                            <li><strong>Admin Role</strong></li>
                            <p>
                                <%=adminInfo.adminRole%>
                            </p>
                            <li><strong>Date Of Birth</strong></li>
                            <p>
                                <%=adminInfo.adminDob%>
                            </p>
                            <li><strong>Emergency Contact Name</strong></li>
                            <p>
                                <%=adminInfo.adminEmergencyName%>
                            </p>
                            <li><strong>Emergency Contact Number</strong></li>
                            <p>
                                <%=adminInfo.adminEmergencyNumber%>
                            </p>
                            <li><strong>Employment date</strong></li>
                            <p>
                                <%=adminInfo.adminEmployDate%>
                            </p>

                        </ul>
                      
                        <div class="table-responsive pt-3">
                        
                        </div>
                    </div>
                </div>
        
            </div>
    
            <%- include('../partials/adminFooter'); -%>
           
        </div>
    
      </div>

    </div>

  </body>
</html>